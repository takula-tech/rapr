{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build All Debug Libraries",
      "type": "shell",
      "command": "cargo build --workspace",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Build All Release Libraries",
      "type": "shell",
      "command": "cargo build -r --workspace",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Run All Unit tests",
      "type": "shell",
      "command": "cargo test --workspace",
      "group": {
        "kind": "test",
        "isDefault": false
      }
    },
    {
      "label": "Examples/empty",
      "type": "shell",
      "command": "cargo run --package pipeline-executor",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
